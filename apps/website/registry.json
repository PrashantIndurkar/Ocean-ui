{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "@ocean-ui",
  "homepage": "https://components.prashantindurkar.in",
  "items": [
    {
      "name": "accordion",
      "type": "registry:ui",
      "title": "Accordion",
      "description": "A vertically stacked set of interactive headings that each reveal a section of content.",
      "dependencies": ["@ark-ui/react", "lucide-react"],
      "registryDependencies": [],
      "files": [
        {
          "path": "../../packages/ui-react/src/components/base/accordion.tsx",
          "type": "registry:ui"
        }
      ],
      "cssVars": {
        "theme": {
          "--animate-accordion-collapse": "accordion-collapse var(--tw-duration, 200ms) ease-out",
          "--animate-accordion-expand": "accordion-expand var(--tw-duration, 200ms) ease-out"
        }
      },
      "css": {
        "@keyframes accordion-collapse": {
          "from": {
            "height": "var(--height)"
          },
          "to": {
            "height": "0"
          }
        },
        "@keyframes accordion-expand": {
          "from": {
            "height": "0"
          },
          "to": {
            "height": "var(--height)"
          }
        }
      }
    }
  ]
}
